{
  "preset": "angular",
  "tagFormat": "superviz-socket-client-${version}",
  "release": {
    "branches": [
      "master",
      {
        "name": "beta",
        "channel": "beta",
        "prerelease": true
      },
      {
        "name": "lab",
        "channel": "lab",
        "prerelease": true
      }
    ]
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    [
      "@semantic-release/release-notes-generator",
      {
        "preset": "angular",
        "parserOpts": {
          "noteKeywords": [
            "BREAKING CHANGE",
            "BREAKING CHANGES",
            "BREAKING"
          ]
        },
        "writerOpts": {
          "commitsSort": [
            "subject",
            "scope"
          ]
        }
      }
    ],
    [
      "@semantic-release/changelog",
      {
        "changelogFile": "CHANGELOG.md"
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": [
          "CHANGELOG.md"
        ]
      }
    ],
    [
      "semantic-release-pub",
      {
        "cli": "dart",
        "publishPub": true
      }
    ]
  ]
}
